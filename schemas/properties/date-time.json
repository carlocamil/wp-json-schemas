{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://raw.githubusercontent.com/johnbillion/wp-json-schemas/trunk/schemas/properties/date-time.json",
	"title": "WP_Date_Time",
	"description": "Timestamp in MySQL DATETIME format (`YYYY-MM-DD hh:mm:ss`).",
	"type": "string",
	"pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2}) ([0-9]{2}):([0-9]{2}):([0-9]{2})$"
}
