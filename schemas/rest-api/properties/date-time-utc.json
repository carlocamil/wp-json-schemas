{
	"$schema": "http://json-schema.org/draft-07/hyper-schema#",
	"$id": "https://raw.githubusercontent.com/johnbillion/wp-json-schemas/trunk/schemas/rest-api/properties/date-time-utc.json",
	"title": "WP_REST_API_Date_Time_UTC",
	"description": "UTC timestamp in IETF RFC 3339 date-time format (`YYYY-MM-DDThh:mm:ss+00:00`).",
	"type": "string",
	"pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})T([0-9]{2}):([0-9]{2}):([0-9]{2})\\+00:00$"
}
